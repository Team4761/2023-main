

moveToHeading(){
    difference = currentHeading - goalHeading;
    moveRotate(difference);
}

moveToCoord(){
    difference = currentpos - goalpos //get distance in straight line
    moveStraight(difference); //with PID values
    useEncoder.setRef.pos;

}


